[user]
	name = Bojan MatiÄ‡
	email = matic.bojan@gmail.com
[core]
	excludesfile = ~/.gitignore_global
[alias]
	d = difftool
	u = fetch --all --prune
	f = fetch --all --prune
	p = push origin --all
	br = branch
	ci = commit
	co = checkout
	st = status
	ff = merge --ff-only
	gg = log --all --graph --oneline --decorate --color
	quick-stats = ! /usr/local/bin/git-quick-stats
[branch]
	autosetuprebase = always
[color]
	ui = true
	status = auto
	branch = auto
[push]
	default = simple
	autoSetupRemote = true
#[credential]
	#helper = osxkeychain
[gui]
	fontui = -family \".SF NS Text\" -size 16 -weight normal -slant roman -underline 0 -overstrike 0
	fontdiff = -family Monaco -size 14 -weight normal -slant roman -underline 0 -overstrike 0
[init]
	defaultBranch = main
